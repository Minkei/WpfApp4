<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--Background Colors-->
    <Color x:Key="primaryBackColor1">#A9BFE6</Color>
    <Color x:Key="primaryBackColor2">#9AB9E1</Color>
    <Color x:Key="secondaryBackColor1">#8AADD6</Color>
    <Color x:Key="secondaryBackColor2">#7D9BCF</Color>


    <!--Window Border Colors-->
    <Color x:Key="winBorderColor1">#4A90E2</Color>
    <Color x:Key="winBorderColor2">#357ABD</Color>
    <Color x:Key="winBorderColor3">#285A8B</Color>

    <!--Color Palette-->
    <SolidColorBrush x:Key="color1" Color="#5C9FDF"/>
    <SolidColorBrush x:Key="color2" Color="#4A90E2"/>
    <SolidColorBrush x:Key="color3" Color="#357ABD"/>
    <SolidColorBrush x:Key="color4" Color="#3C6478"/>
    <SolidColorBrush x:Key="color5" Color="#A4C2F4"/>
    <SolidColorBrush x:Key="color6" Color="#C7D9F1"/>
    <SolidColorBrush x:Key="color7" Color="#D4E3FA"/>
    <SolidColorBrush x:Key="color8" Color="#2B4C7E"/>
    <SolidColorBrush x:Key="color9" Color="#1E3A6F"/>
    <SolidColorBrush x:Key="color10" Color="#8AB6FF"/>
    <SolidColorBrush x:Key="color11" Color="#99CCFF"/>

    <!--Font Colors-->
    <SolidColorBrush x:Key="titleColor1" Color="#0F2647"/>
    <SolidColorBrush x:Key="titleButtonColor" Color="Black"/>
    <SolidColorBrush x:Key="titleBackgroundColor1" Color="Black"/>
    <SolidColorBrush x:Key="plainTextColor1" Color="#2B4C7E"/>
    <SolidColorBrush x:Key="plainTextColor2" Color="#3C6478"/>
    <SolidColorBrush x:Key="plainTextColor3" Color="#5C9FDF"/>

    <!--Panel Colors-->
    <SolidColorBrush x:Key="panelColor" Color="#F0F4FF"/>
    <SolidColorBrush x:Key="panelOverColor" Color="#D9E6F6"/>
    <SolidColorBrush x:Key="panelActiveColor" Color="#C7D9F1"/>

    <!--Button Colors-->
    <SolidColorBrush x:Key="buttonColor" Color="#C7D9F1"/>
    <SolidColorBrush x:Key="buttonHoverColor" Color="#357ABD"/>
    <SolidColorBrush x:Key="buttonActiveColor" Color="LightGreen"/>

    <!--TextBox Colors-->
    <SolidColorBrush x:Key="textBoxBorderColor" Color="#4A90E2"/>
    <SolidColorBrush x:Key="textBoxBackgroundColor" Color="#F0F4FF"/>
    <SolidColorBrush x:Key="textBoxTextColor" Color="#0F2647"/>
</ResourceDictionary>
